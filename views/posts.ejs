<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link rel="stylesheet" href="/stylesheets/style.css" />
    <link
      rel="stylesheet"
      href="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0/css/materialize.min.css"
    />
    <script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0/js/materialize.min.js"></script>
    <title>Document</title>
  </head>
  <body>
    <nav>
      <div class="nav-wrapper">
        <a href="#" class="brand-logo">PK's Blog</a>
        <ul id="nav-mobile" class="right hide-on-med-and-down">
          <li><a href="#">New Post</a></li>
          <li><a href="/blog/owner/posts/logout">Logout</a></li>
        </ul>
      </div>
    </nav>
    <div class="row">
      <div class="input-field col s12">
        <select>
          <option value="all">All Posts</option>
          <option value="published">Published Posts</option>
          <option value="unpublished">Unpublished Posts</option>
        </select>
        <label>Currently Showing</label>
      </div>
    </div>
  </body>
  <script>
    document.addEventListener("DOMContentLoaded", function () {
      var elems = document.querySelectorAll("select");
      var instances = M.FormSelect.init(elems, "input-field");
    });
  </script>
</html>
